package com.test;

import java.util.ArrayList;
import java.util.List;

public class Test {

	
	public static void main(String[] args) {
		
		
		
		List<Integer> l = new ArrayList<>();
		
		l.add(1);
		l.add(10);
		l.add(15);
		l.add(22);
		l.add(30);
		
		
		for(int a : l)
		
			
			if(a%2 != 0)
			{
				
				System.out.println(a);
		}
		
		
		
		
	}
}
