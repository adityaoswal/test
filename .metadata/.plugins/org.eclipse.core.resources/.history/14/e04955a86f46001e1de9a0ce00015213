public class Pattern {

	public static void main(String[] args) {

	Scanner sc = new Scanner(System.in);
	
		System.out.println("enter the number");
		int x = sc.nextInt();
	
		int rem;
		int ne=1;
		while(x!=0)
		{
		
		rem = x%10;
		 ne =   rem*10;
		x =x/10;
		
			
			
			
			
		}
		
		System.out.println(ne);
	}
	
	
}
