package com.test;

import java.util.ArrayList;
import java.util.List;

public class Test {

	
	public static void main(String[] args) {
		
		
		
		List<Integer> l = new ArrayList<>();
		
		l.add(99);
		l.add(10);
		l.add(50);
		l.add(22);
		l.add(30);
		
		
	   l.stream().filter(n -> n %2 == 0).forEach(n -> System.out.println(n););
		
	   
		
		
	}
}
