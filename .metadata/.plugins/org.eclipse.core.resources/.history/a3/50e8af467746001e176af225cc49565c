package mixofprimeandreverse;

import java.util.Comparator;
final public class Employee  {

private final int aid;
private final String name;
public Employee(int aid, String name) {
	super();
	this.aid = aid;
	this.name = name;
}
public int getAid() {
	return aid;
}
public String getName() {
	return name;
}
@Override
public String toString() {
	return "Employee [aid=" + aid + ", name=" + name + "]";
}





	
}
