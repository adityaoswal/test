package collect;

import java.util.ArrayList;
import java.util.HashMap;
import java.util.List;
import java.util.Map;

public class Test {

	public static void main(String[] args) {
		
		
		Map<String, Map<Integer, List<Employee>>> map = new HashMap<>();
		
		Employee e = new Employee(1,"adi","pune");
		
		Employee e1 = new Employee(2, "shubh", "nagpur");
		
		
		List<Employee> list = new ArrayList<>();
		
		list.add(e);
		list.add(e1);
		
		Map<Integer, List<Employee>> innermap = new HashMap<>();
		
		innermap.put(1, list);
		
		
		map.put("goku", innermap);
		
	   map.forEach((key,inner)->{
		   
		   System.out.println(key);
		   
		   inner.forEach((innerkey,employ)->{
			   
			   System.out.println(innerkey);
			   
			   employ.forEach((emp)->{
				   
				   emp.getEid();
				   emp.getEname();
				   emp.getAddr()
				   
			   });
			   
			   
		   });
		   
		   
	   });
		
		
		
		
		
	
		
		
	}
	
}
