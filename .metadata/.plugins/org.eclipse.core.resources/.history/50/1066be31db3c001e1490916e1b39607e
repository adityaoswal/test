package mixofprimeandreverse;

import java.util.Scanner;

public class Test {

	public static void main(String[] args) {
		

	int xop [] = {1,2,3};
	
	try {
		
		int res = xop[4];
		
		System.out.println(res);
		
	}
		catch(ArrayIndexOutOfBoundsException e)
		{
			
			System.out.println(e.getMessage());
			
		}
		
	}
	
	
}
