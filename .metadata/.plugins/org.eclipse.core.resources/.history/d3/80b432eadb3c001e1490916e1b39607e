package mixofprimeandreverse;

import java.util.Scanner;

public class Test {

	public static void main(String[] args) {
		

	String s[]= {"hello"};
	
	try {
		
		int res = s.length;
		
		System.out.println(res);
		
	}
		catch(ArrayIndexOutOfBoundsException e)
		{
			
			System.out.println(e.getMessage());
			
		}
		
	}
	
	
}
