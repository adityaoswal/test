package patternpractice;
import java.util.*;
import java.util.Map.Entry;
import java.util.concurrent.CopyOnWriteArrayList;
import java.util.stream.Stream;
import java.util.*;
public class Pattern {

	public static void main(String[] args) {

Map<Integer, String> maharashtra = new HashMap<>();

maharashtra.put(1, "mumbai");
maharashtra.put(2, "nagpur");

Map<Integer, String> kerala = new HashMap<>();
kerala.put(1, "cochi");
kerala.put(2, "munar");

Map<Integer, String> usstate1 = new HashMap<>();

usstate1.put(1, "uscity1");
usstate1.put(2, "uscity2");

Map<Integer, String> usstate2 = new HashMap<>();
usstate2.put(1, "uscity3");
usstate2.put(2, "uscity2");

Map<Integer,Map<Integer, String>> india = new HashMap<>();
india.put(1, maharashtra);
india.put(2, kerala);

Map<Integer, Map<Integer, String>> us = new HashMap<>();
us.put(1, usstate2);
us.put(2, usstate1);

Map<Integer, Map<Integer, Map<Integer, String>>> world = new HashMap<>();
world.put(1, us);
world.put(2, india);

System.out.println(world);


=
	
	
	}
	
	
}
